package com.sheshu;

public class OCTCoding21 {
	static void pyramid() {
		for(int i=1;i<=5;i++) {
			for(int k=0;k<5-i;k++) {
				System.out.print("  ");
			}
			for(int j=0;j<2*i-1;j++) {
				System.out.print("* ");
			}
			System.out.println();
		}
	}
	public static void main(String[] args) {
		pyramid();
	}

}
